{
  "name": "veek",
  "version": "0.1.0",
  "author": "Rodrigo Oler <roodrigoprogrammer@gmail.com>",
  "dependencies": {
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-scripts": "1.1.0"
  },
  "devDependencies": {
    "husky": "0.14.3",
    "lint-staged": "6.1.0",
    "prettier": "1.10.2"
  },
  "license": "UNLICENCED",
  "lint-staged": {
    "*.js": [
      "prettier",
      "git add"
    ]
  },
  "main": "build/index.html",
  "private": true,
  "repository": "git@github.com:rodrigooler/veek.git",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "prettier": "./node_modules/.bin/prettier --write --single-quote true --trailing-comma all --print-width 80 --write 'src/**/*.js'",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "upg": "yarn upgrade-interactive"
  }
}
